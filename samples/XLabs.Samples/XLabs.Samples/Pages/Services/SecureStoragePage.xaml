<?xml version="1.0" encoding="utf-8" ?>
<ContentPage x:Class="XLabs.Samples.Pages.Services.SecureStoragePage"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <ContentPage.Content>
        <StackLayout>
            <Label HeightRequest="45" Text="Key" />
            <Entry x:Name="KeyEntry" />
            <Label Text="Data" />
            <Entry x:Name="DataEntry" />
            <Button x:Name="SaveButton" Text="Save" />
            <Button x:Name="DeleteButton" Text="Delete" />
            <Button x:Name="LoadButton" Text="Load" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>